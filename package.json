{
	"name": "workers-hbs",
	"version": "0.1.0",
	"main": "dist/index.js",
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230404.0",
		"@types/jest": "^29.5.0",
		"esbuild": "^0.17.16",
		"jest": "^29.5.0",
		"rimraf": "^5.0.0",
		"ts-jest": "^29.1.0",
		"typescript": "^4.9.4",
		"wrangler": "^2.15.0"
	},
	"scripts": {
		"build": "rimraf dist && npm run build:types && npm run build:js",
		"build:dev": "rimraf dist && npm run build:types && npm run build:js-dev",
		"build:js": "npx esbuild --bundle --minify --platform=node --outfile=dist/index.js ./src/index.js",
		"build:js-dev": "npx esbuild --bundle --watch --platform=node --outfile=dist/index.js ./src/index.js",
		"build:typesxx": "tsc -p . --excludeFiles \"*.test.ts\"",
		"build:types": "mkdir dist && echo \"declare module 'workers-hbs';\" > dist/index.d.ts",
		"local": "wrangler dev --local main.js --compatibility-date=2023-04-12 --node-compat",
		"test": "jest --rootDir=tests/"
	},
	"dependencies": {
		"@handlebars/parser": "^2.1.0",
		"handlebars": "^4.7.7"
	}
}
